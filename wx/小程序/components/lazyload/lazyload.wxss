.img{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  border-radius:3px;
}
.mask{
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  width: 100%;
  background: url('data:image/png;base64,') no-repeat 0 0 ;
  background-size: 100% 100%;
}
.container{
  position: relative;
}

.loaded .mask{
  transition: all 0.3s;
  opacity: 0;
}